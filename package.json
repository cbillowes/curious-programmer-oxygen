  "main": "n/a",
  "scripts": {
    "clean": "rm -rf .cache && rm -rf public",
    "format": "prettier '**/*.{js,jsx}' --write --loglevel debug",
    "markdown:writing": "write-good $(glob 'content/posts/**/*.md')",
    "markdown:lint": "remark content/posts/",
    "ready": "npm run clean && npm run format",
    "develop": "npm run ready && gatsby develop --host localhost --port 8081",
    "build": "npm run ready && gatsby build --prefix-paths",
    "serve": "npm run build && gatsby serve"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
